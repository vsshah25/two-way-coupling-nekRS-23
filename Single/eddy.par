[GENERAL] 
polynomialOrder = 7
#startFrom = "restart.fld"
numSteps = 20000
dt = 1e-03
timeStepper = tombo2
writeInterval = 100

[PRESSURE]
residualTol = 1e-08

[VELOCITY]
#boundaryTypeMap = v  
residualTol = 1e-12
density = 1.0
viscosity = 0.01 

[TEMPERATURE]
#boundaryTypeMap = v  
solver = none

[CASEDATA]
# inject nParticlesInject particles every injectionFrequency time steps
nParticlesInject = 1024
injectionFrequency = 50
Stk = 0.05
